{
  "name": "coinsmarket",
  "version": "0.0.1",
  "scripts": {
    "test:unit": "echo \"Error: no test specified\" && exit 1",
    "lint": "node_modules/.bin/npm-run-all --parallel lint:*",
    "lint:js": "node_modules/.bin/eslint \"**/*.js\" -c .eslintrc",
    "fix:lint:js": "npm run lint:js -- --fix",
    "lint:sass": "node_modules/.bin/sass-lint -c .sass-lint.yml -v -q",
    "setup:refresh": "npm prune && npm install && npm update",
    "prepush": "npm run lint",
    "start": "node_modules/.bin/cross-env BABEL_ENV=local HOST_PROTOCOL=http NODE_ENV=local NODE_PORT=3001 HOST_ADDRESS=local-coinsmarket.com node ./scripts/start.js",
    "start:environments": "node_modules/.bin/cross-env BABEL_ENV=local HOST_PROTOCOL=http NODE_PORT=3001 HOST_ADDRESS=local-coinsmarket.com node ./scripts/start.js",
    "start:dev": "node_modules/.bin/cross-env NODE_ENV=dev npm run start:environments",
    "build": "node_modules/.bin/cross-env BABEL_ENV=es NODE_ENV=local HOST_PROTOCOL=http NODE_PORT=3001 HOST_ADDRESS=local-coinsmarket.com ./node_modules/.bin/webpack --config ./webpack-config/webpack.config.local.js --progress",
    "build:stats": "node_modules/.bin/cross-env STATS=true npm run build",
    "build:environments": "node_modules/.bin/cross-env BABEL_ENV=es HOST_PROTOCOL=https NODE_PORT=443 ./node_modules/.bin/webpack --config ./webpack-config/webpack.config.environments.js --progress",
    "build:dev": "node_modules/.bin/cross-env NODE_ENV=dev HOST_ADDRESS=coinsmarket-dev.com npm run build:environments",
    "build:dev:stats": "node_modules/.bin/cross-env STATS=true npm run build:dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kazazor/coinsmarket.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kazazor/coinsmarket/issues"
  },
  "dependencies": {
    "core-js": "2.4.1"
  },
  "devDependencies": {
    "autoprefixer": "6.7.7",
    "babel-core": "6.24.0",
    "babel-eslint": "7.2.1",
    "babel-jest": "19.0.0",
    "babel-loader": "6.4.1",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-syntax-trailing-function-commas": "6.22.0",
    "babel-plugin-transform-class-properties": "6.23.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-react": "6.23.0",
    "clean-webpack-plugin": "0.1.16",
    "cross-env": "3.2.4",
    "css-loader": "0.27.3",
    "eslint": "3.18.0",
    "eslint-plugin-no-unsafe-innerhtml": "1.0.16",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-react": "6.10.3",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.10.1",
    "html-webpack-inline-chunk-plugin": "1.1.1",
    "html-webpack-plugin": "2.28.0",
    "husky": "0.13.3",
    "interpolate-loader-options-webpack-plugin": "0.0.1",
    "jest": "19.0.2",
    "jest-cli": "19.0.2",
    "json-loader": "0.5.4",
    "node-sass": "4.5.1",
    "npm-run-all": "4.0.2",
    "opn": "4.0.2",
    "postcss-loader": "1.3.3",
    "raw-loader": "0.5.1",
    "sass-lint": "1.10.2",
    "sass-loader": "6.0.3",
    "script-ext-html-webpack-plugin": "1.7.1",
    "style-loader": "0.16.1",
    "svgo-loader": "1.2.1",
    "url-loader": "0.5.8",
    "webpack": "2.4.1",
    "webpack-bundle-analyzer": "2.4.0",
    "webpack-dashboard": "0.3.0",
    "webpack-dev-server": "2.4.2",
    "yaml-loader": "0.4.0"
  },
  "jest": {
    "moduleNameMapper": {
      "^.*[.](yml|yaml)$": "<rootDir>/jestMappers/yamlMapper.js",
      "^.*[.](scss)$": "<rootDir>/jestMappers/scssMapper.js",
      "^config": "<rootDir>/webpack-config/env-configs/local.js"
    },
    "testPathIgnorePatterns": [
      "/src/{assets,styles}/"
    ],
    "roots": [
      "<rootDir>/specs"
    ],
    "testRegex": ".*.spec\\.js$",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  }
}
